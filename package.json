{
  "name": "datamovesme-sqlcourse",
  "repository": {
    "type": "git",
    "url": "https://github.com/KristenKehrer/datamovesme-sqlcourse.git"
  },
  "version": "0.0.0",
  "scripts": {
    "fix-gh-creds": "git config --global credential.helper wincred",
    "git-kristen": "git remote set-url origin https://kristenkehrer@gmail.com:Change4153@github.com/KristenKehrer/datamovesme-sqlcourse.git",
    "git-paul": "git remote set-url origin https://github.com/pkehrer/datamovesme-sqlcourse.git",
    "make-db": "node sql/createsql",
    "make-asset-dir": "mkdirp dist/datamovesme-sqlcourse/assets/",
    "copy-db-dist": "ncp sql/sqlcourse.db dist/datamovesme-sqlcourse/assets/sqlcourse.db",
    "copy-db-src": "ncp sql/sqlcourse.db src/assets/sqlcourse.db",
    "copy-db": "run-p copy-db-dist copy-db-src",
    "db": "run-s make-db make-asset-dir copy-db",
    "gh-build-paul": "ng build --prod --base-href https://pkehrer.github.io/datamovesme-sqlcourse/",
    "gh-build-kristen": "ng build --prod --base-href https://kristenkehrer.github.io/datamovesme-sqlcourse/",
    "gh-deploy": "angular-cli-ghpages -S --dir=dist/datamovesme-sqlcourse",
    "deploy-kristen": "run-s git-kristen db gh-build-kristen gh-deploy",
    "deploy-paul": "run-s git-paul db gh-build-paul gh-deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@ctrl/ngx-codemirror": "^1.3.9",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "ag-grid-angular": "^19.1.2",
    "ag-grid-community": "^19.1.4",
    "angular-split": "^2.0.0",
    "bootstrap": "^4.1.3",
    "codemirror": "^5.42.0",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.0",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5",
    "rxjs": "~6.3.3",
    "sql.js": "^0.5.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/file-saver": "^2.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "angular-cli-ghpages": "^0.5.3",
    "bluebird": "^3.5.3",
    "codelyzer": "~4.5.0",
    "csvtojson": "^2.0.8",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "protractor": "~5.4.0",
    "sqlite3": "^4.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}