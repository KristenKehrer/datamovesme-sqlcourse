{
  "name": "datamovesme-sqlcourse",
  "repository": {
    "type": "git",
    "url": "https://github.com/KristenKehrer/datamovesme-sqlcourse.git"
  },
  "alternateRepository": {
    "type": "git",
    "url": "https://github.com/pkehrer/datamovesme-sqlcourse.git"
  },
  "version": "0.0.0",
  "scripts": {
    "fix-gh-creds": "git config --global credential.helper wincred",
    "git-kristen": "node load-git --kristen",
    "git-paul": "node load-git --paul",
    "make-db": "node sql/createsql",
    "make-asset-dir": "mkdirp dist/datamovesme-sqlcourse/assets/",
    "copy-db-dist": "ncp sql/sqlcourse.db dist/datamovesme-sqlcourse/assets/sqlcourse.db",
    "copy-db-src": "ncp sql/sqlcourse.db src/assets/sqlcourse.db",
    "copy-db": "run-p copy-db-dist copy-db-src",
    "db": "run-s make-db make-asset-dir copy-db",
    "gh-build-paul": "ng build --prod --base-href https://pkehrer.github.io/datamovesme-sqlcourse/",
    "gh-build-kristen": "ng build --prod --base-href https://kristenkehrer.github.io/datamovesme-sqlcourse/",
    "gh-deploy": "angular-cli-ghpages -S --dir=dist/datamovesme-sqlcourse",
    "deploy-kristen": "run-s git-kristen gh-build-kristen db gh-deploy",
    "deploy-paul": "run-s git-paul gh-build-paul db gh-deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.5",
    "@angular/common": "^7.2.5",
    "@angular/compiler": "^7.2.5",
    "@angular/core": "^7.2.5",
    "@angular/forms": "^7.2.5",
    "@angular/platform-browser": "^7.2.5",
    "@angular/platform-browser-dynamic": "^7.2.5",
    "@angular/router": "^7.2.5",
    "@ctrl/ngx-codemirror": "^1.3.9",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "ag-grid-angular": "^19.1.2",
    "ag-grid-community": "^19.1.4",
    "angular-split": "^2.0.1",
    "bootstrap": "^4.2.1",
    "codemirror": "^5.42.2",
    "core-js": "^2.6.1",
    "file-saver": "^2.0.0",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "~6.3.3",
    "sql.js": "^0.5.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.2",
    "@angular/cli": "^7.3.2",
    "@angular/compiler-cli": "^7.2.5",
    "@angular/language-service": "^7.2.5",
    "@types/file-saver": "^2.0.0",
    "@types/jasmine": "^2.8.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.120",
    "@types/node": "^8.10.39",
    "angular-cli-ghpages": "^0.5.3",
    "bluebird": "^3.5.3",
    "codelyzer": "~4.5.0",
    "csvtojson": "^2.0.8",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "protractor": "^5.4.2",
    "sqlite3": "^4.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
